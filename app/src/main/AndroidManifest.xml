<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rudrik.quotes"
    android:versionCode="2"
    android:versionName="1.1">

    <permission android:name="android.permission.SEND_SMS"></permission>


    <!-- Airpush Permission's -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <!-- Airpush Permission's End -->


    <application
        android:icon="@drawable/icon"
        android:label="@string/app_name">
        <activity
            android:name=".Main"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TipviewActivity"></activity>
        <activity android:name=".FavList"></activity>
        <activity android:name=".Information"></activity>
        <activity android:name=".FavTipView"></activity>

        <!--airpush Start-->
        <!-- This activity is required for all ads. -->
        <activity
            android:name="com.mqnvnfx.itwsdvr70223.AdActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <!-- End of the required declaration -->
        <activity
            android:name="com.mqnvnfx.itwsdvr70223.BrowserActivity"
            android:configChanges="orientation|screenSize" />

        <!-- End of Airpush Manifest declaration -->
        <!--airpush end-->

    </application>
</manifest>